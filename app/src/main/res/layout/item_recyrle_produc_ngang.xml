<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="194dp"    android:layout_height="255dp"    android:layout_margin="10dp"app:cardBackgroundColor="@color/white"    android:orientation="vertical"    app:cardCornerRadius="20dp"    app:cardElevation="5dp">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <FrameLayout            android:id="@+id/frameLayout2"            android:layout_width="match_parent"            android:layout_height="160dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <ImageView                android:id="@+id/item_horizontal_produc_imgshow"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:src="@drawable/tree2" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="48dp"                android:layout_gravity="bottom"                android:alpha="0.5"                android:background="#E9E9E9" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_gravity="bottom"                android:orientation="vertical"           android:padding="10dp">                <TextView                    android:id="@+id/item_horizontal_produc_tvTenSanPham"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="XƯƠNG RỒNG CẢNH"                    android:textColor="#2C2C2C"                    android:textSize="10sp"                    android:textStyle="bold" />                <TextView                    android:id="@+id/item_horizontal_produc_tvGiaSanPham"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="120.000đ"                    android:textColor="#8BB14F"                    android:textSize="10sp"                    android:textStyle="bold" />            </LinearLayout>        </FrameLayout>        <TextView            android:id="@+id/item_horizontal_produc_tvXuatXuSanPham"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="10dp"            android:layout_marginTop="4dp"            android:text="Xuất xứ: Đà Lạt"            android:textColor="#525252"            android:textSize="6sp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/frameLayout2" />        <TextView            android:id="@+id/item_horizontal_produc_tvLoaiSanPham"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="10dp"            android:text="Loại: cây cảnh vườn"            android:textColor="#525252"            android:textSize="6sp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="0"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/item_horizontal_produc_tvXuatXuSanPham" />        <ImageView            android:id="@+id/item_horizontal_produc_img_btnAddToCard"            android:layout_width="30dp"            android:layout_height="30dp"            android:layout_marginBottom="8dp"            android:src="@drawable/ic_add_to_card"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>