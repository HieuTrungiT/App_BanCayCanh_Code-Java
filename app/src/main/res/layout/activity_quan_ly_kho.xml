<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context=".Activity.datNhanh_Activity">    <FrameLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical">            <androidx.constraintlayout.widget.ConstraintLayout                android:layout_width="match_parent"                android:layout_height="60dp"                android:layout_gravity="center|top">                <ImageView                    android:id="@+id/quanLyKho_img_btn_back"                    android:layout_width="30dp"                    android:layout_height="30dp"                    android:layout_marginLeft="15dp"                    android:layout_marginTop="10dp"                    android:layout_marginBottom="10dp"                    android:src="@drawable/ic_back"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toTopOf="parent" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginRight="15dp"                    android:text="Quản lý kho"                    android:textColor="#A0A0A0"                    android:textSize="13sp"                    android:textStyle="bold"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toEndOf="parent"                    app:layout_constraintTop_toTopOf="parent" />            </androidx.constraintlayout.widget.ConstraintLayout>            <View                android:layout_width="match_parent"                android:layout_height="2dp"                android:alpha="0.5"                android:background="@color/green_brand" />            <LinearLayout                android:id="@+id/linearLayout11"                android:layout_width="match_parent"                android:layout_height="32dp"                android:layout_marginTop="15dp"                android:layout_marginBottom="15dp"                android:paddingLeft="15dp">                <EditText                    android:id="@+id/quanLyKho_edt_search"                    android:layout_width="200dp"                    android:layout_height="match_parent"                    android:background="@drawable/broder_stroke_green"                    android:hint="Search"                    android:paddingLeft="10dp"                    android:textSize="15dp" />                <ImageView                    android:id="@+id/quanLyKho_img_btn_search"                    android:layout_width="32dp"                    android:layout_height="32dp"                    android:layout_marginLeft="10dp"                    android:background="@drawable/btn_broder_green"                    android:padding="7dp"                    android:src="@drawable/search" />            </LinearLayout>            <FrameLayout                android:layout_width="match_parent"                android:layout_height="match_parent">                <ImageView                    android:id="@+id/quanLyKho_img_showifNullProduc"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:src="@drawable/ic_box"                    android:visibility="invisible" />                <androidx.recyclerview.widget.RecyclerView                    android:id="@+id/quanLyKho_rscv_showSanPham"                    android:layout_width="match_parent"                    android:layout_height="match_parent" />                <com.google.android.material.floatingactionbutton.FloatingActionButton                    android:id="@+id/quanLyKho_floatAsBtn_themSanPham"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="end|bottom"                    android:layout_margin="20dp"                    android:backgroundTint="#BFC269"                    android:src="@drawable/ic_baseline_add_24" />            </FrameLayout>        </LinearLayout>        <LinearLayout            android:id="@+id/gioHang_costLout_loanding"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="@color/white"            android:gravity="center">            <TextView                android:id="@+id/sanPham_tv_textAnim"                android:layout_width="wrap_content"                android:layout_height="wrap_content" />            <com.airbnb.lottie.LottieAnimationView                android:id="@+id/sanPham_anim_Loanding"                android:layout_width="100dp"                android:layout_height="100dp"                android:layout_gravity="center"                app:lottie_autoPlay="true"                app:lottie_loop="true"                app:lottie_rawRes="@raw/loading_sp" />        </LinearLayout>    </FrameLayout></LinearLayout>